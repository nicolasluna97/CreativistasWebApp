{
  "name": "creactivistas-tests",
  "version": "0.9.9",
  "description": "Aplicacion web de Creactivistas, que contiene el test de personalidad Zoom basado en el MBTI, y el test open source Big 5.",
  "private": true,
  "author": {
    "name": "Alejandro R Buteler",
    "email": "abuteler@enneagonstudios.com",
    "url": "https://github.com/abuteler"
  },
  "contributors": [
    {
      "name": "Open source project",
      "email": "jonas.enge@t-fk.no",
      "url": "https://github.com/telemark"
    }
  ],
  "main": "app.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "audit": "npm audit",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider node app.js",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "cleanup": "rm -rf .next",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install",
    "deploy": "now && now alias"
  },
  "keywords": [
    "mbti",
    "bigfive",
    "personalidad"
  ],
  "license": "MIT",
  "dependencies": {
    "@alheimsins/b5-johnson-120-ipip-neo-pi-r": "5.0.5",
    "@alheimsins/b5-result-text": "3.0.4",
    "@alheimsins/react-google-charts": "1.6.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@sendgrid/mail": "^7.2.2",
    "axios": "0.19.2",
    "b5-calculate-score": "1.0.4",
    "babel-eslint": "10.1.0",
    "chart.js": "^2.9.4",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "helmet": "3.23.0",
    "iso-639-1": "2.1.3",
    "mongodb": "^4.14.0",
    "next": "^13.4.0",
    "next-offline": "^5.0.3",
    "next-routes": "1.4.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^2.11.1",
    "react-copy-to-clipboard": "5.0.2",
    "react-dom": "^18.2.0",
    "react-google-charts": "3.0.15",
    "react-icons": "3.10.0",
    "request-country": "0.1.5"
  },
  "devDependencies": {
    "@vercel/node": "^1.7.4",
    "cross-env": "^7.0.3"
  }
}